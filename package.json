{
  "name": "xm-fns",
  "version": "1.1.0",
  "description": "一个前端的常用方法的工具库，包含CSS的、JavaScript的…… A front-end tool library for common methods, includes CSS, JavaScript ...",
  "main": "dist/main.js",
  "scripts": {
    "build": "rollup -c",
    "test": "jest",
    "style": "eslint main.js src/**/*.js",
    "validate": "npm run build && npm run style && npm run test",
    "genDocs": "node ./src/genDocsMD"
  },
  "keywords": [],
  "author": "littleHiuman",
  "license": "MIT",
  "files": [
    "dist",
    "typings"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/littleHiuman/xm-fns.git"
  },
  "devDependencies": {
    "@babel/plugin-transform-modules-commonjs": "^7.16.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.16.0",
    "eslint-plugin-import": "^2.26.0",
    "jest": "^27.4.3",
    "rewire": "^5.0.0",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-terser": "^7.0.2",
    "typescript": "^4.9.5"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    },
    "cz-customizable": {
      "config": ".cz-config.js"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "typings": "typings/main.d.ts"
}
